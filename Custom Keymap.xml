<keymap version="1" name="Custom Keymap" parent="$default">
  <action id="ActivateCommitToolWindow">
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="ActivateDebugToolWindow">
    <keyboard-shortcut first-keystroke="alt w" second-keystroke="alt d" />
  </action>
  <action id="ActivateProblemsViewToolWindow">
    <keyboard-shortcut first-keystroke="alt w" second-keystroke="alt p" />
  </action>
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="alt e" />
  </action>
  <action id="ActivateRunToolWindow">
    <keyboard-shortcut first-keystroke="alt w" second-keystroke="alt r" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
  <action id="ActivateVersionControlToolWindow">
    <keyboard-shortcut first-keystroke="alt w" second-keystroke="alt g" />
  </action>
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="ctrl alt q" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="alt q" />
  </action>
  <action id="CppIncludesHierarchy" />
  <action id="Debug">
    <keyboard-shortcut first-keystroke="shift f9" />
    <keyboard-shortcut first-keystroke="alt r" second-keystroke="alt d" />
  </action>
  <action id="EditorContextInfo" />
  <action id="Git.Branches">
    <keyboard-shortcut first-keystroke="shift ctrl back_quote" />
    <keyboard-shortcut first-keystroke="alt g" second-keystroke="alt b" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="button2" />
    <keyboard-shortcut first-keystroke="alt n" second-keystroke="alt u" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="ctrl alt b" />
    <mouse-shortcut keystroke="control alt button1" />
    <keyboard-shortcut first-keystroke="alt n" second-keystroke="alt i" />
  </action>
  <action id="Jdbc.OpenConsole.New" />
  <action id="Jdbc.OpenConsole.New.Generate" />
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="alt left" />
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="alt r" second-keystroke="alt t" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="alt r" second-keystroke="alt n" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt w" second-keystroke="alt w" />
  </action>
  <action id="Stop">
    <keyboard-shortcut first-keystroke="ctrl f2" />
    <keyboard-shortcut first-keystroke="alt r" second-keystroke="alt s" />
  </action>
  <action id="TabShiftActions.MoveFocusLeft">
    <keyboard-shortcut first-keystroke="shift alt h" />
  </action>
  <action id="TabShiftActions.MoveFocusRight">
    <keyboard-shortcut first-keystroke="shift alt l" />
  </action>
  <action id="TabShiftActions.ShiftLeft">
    <keyboard-shortcut first-keystroke="shift alt open_bracket" />
    <keyboard-shortcut first-keystroke="alt w" second-keystroke="alt h" />
  </action>
  <action id="TabShiftActions.ShiftRight">
    <keyboard-shortcut first-keystroke="shift alt close_bracket" />
    <keyboard-shortcut first-keystroke="alt w" second-keystroke="alt l" />
  </action>
  <action id="ToggleRenderedDocPresentation" />
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="alt w" second-keystroke="alt u" />
  </action>
  <action id="VimPluginToggle">
    <keyboard-shortcut first-keystroke="shift back_quote" />
  </action>
  <action id="context.load" />
  <action id="copilot.disposeInlays" />
</keymap>
